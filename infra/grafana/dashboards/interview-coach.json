{
  "dashboard": {
    "id": null,
    "title": "DS Interview Coach Monitoring",
    "tags": ["interview-coach", "rag", "monitoring"],
    "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0},
        "type": "graph",
        "title": "Query Rate",
        "targets": [
          {
            "expr": "rate(query_count[5m])",
            "legendFormat": "Queries/min"
          }
        ]
      },
      {
        "id": 2,
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 0},
        "type": "graph",
        "title": "Response Time Percentiles",
        "targets": [
          {
            "expr": "histogram_quantile(0.5, response_time_ms)",
            "legendFormat": "p50"
          },
          {
            "expr": "histogram_quantile(0.95, response_time_ms)",
            "legendFormat": "p95"
          },
          {
            "expr": "histogram_quantile(0.99, response_time_ms)",
            "legendFormat": "p99"
          }
        ]
      },
      {
        "id": 3,
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 8},
        "type": "graph",
        "title": "Satisfaction Rate",
        "targets": [
          {
            "expr": "avg(satisfaction_rate)",
            "legendFormat": "Satisfaction %"
          }
        ]
      },
      {
        "id": 4,
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 8},
        "type": "piechart",
        "title": "Query Distribution by Category",
        "targets": [
          {
            "expr": "sum by (category) (query_count)",
            "legendFormat": "{{category}}"
          }
        ]
      },
      {
        "id": 5,
        "gridPos": {"h": 8, "w": 8, "x": 0, "y": 16},
        "type": "stat",
        "title": "Total Queries Today",
        "targets": [
          {
            "expr": "sum(increase(query_count[24h]))",
            "legendFormat": ""
          }
        ]
      },
      {
        "id": 6,
        "gridPos": {"h": 8, "w": 8, "x": 8, "y": 16},
        "type": "stat",
        "title": "Active Sessions",
        "targets": [
          {
            "expr": "count(distinct by (session_id) (session_id))",
            "legendFormat": ""
          }
        ]
      },
      {
        "id": 7,
        "gridPos": {"h": 8, "w": 8, "x": 16, "y": 16},
        "type": "stat",
        "title": "Avg Response Time",
        "targets": [
          {
            "expr": "avg(response_time_ms)",
            "legendFormat": ""
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "ms"
          }
        }
      },
      {
        "id": 8,
        "gridPos": {"h": 8, "w": 24, "x": 0, "y": 24},
        "type": "table",
        "title": "Recent Problematic Queries",
        "targets": [
          {
            "expr": "topk(10, negative_feedback_count)",
            "format": "table",
            "legendFormat": ""
          }
        ]
      }
    ],
    "refresh": "10s",
    "time": {
      "from": "now-6h",
      "to": "now"
    }
  },
  "overwrite": true
}